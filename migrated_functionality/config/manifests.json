{
  "namespace": "mas-ecosystem",
  "deployments": [
    {
      "name": "ollama-deployment",
      "replicas": 1,
      "image": "ollama/ollama",
      "ports": [
        11434
      ]
    },
    {
      "name": "chromadb-deployment",
      "replicas": 1,
      "image": "chromadb/chroma",
      "ports": [
        8000
      ]
    }
  ],
  "services": [
    {
      "name": "ollama-service",
      "type": "ClusterIP",
      "ports": [
        {
          "port": 11434,
          "targetPort": 11434
        }
      ]
    },
    {
      "name": "chromadb-service",
      "type": "ClusterIP",
      "ports": [
        {
          "port": 8000,
          "targetPort": 8000
        }
      ]
    }
  ]
}