{
  "name": "vercel",
  "display_name": "Vercel",
  "description": "VERCEL MCP server for IZA OS ecosystem",
  "deployment": {
    "type": "docker",
    "image": "ghcr.io/klavis-ai/vercel-mcp-server:latest",
    "port": 5932,
    "environment": {
      "SERVER_NAME": "VERCEL",
      "IZA_OS_INTEGRATION": "true",
      "LOG_LEVEL": "info"
    }
  },
  "oauth": {
    "enabled": true,
    "scopes": [
      "read",
      "write"
    ],
    "redirect_uri": "https://iza-os.com/oauth/vercel/callback"
  },
  "capabilities": [
    "deployment",
    "domain_management",
    "analytics"
  ],
  "iza_os_integration": {
    "agent_access": [
      "execution_agent",
      "monitoring_agent"
    ],
    "workflow_integration": true,
    "monitoring_enabled": true
  }
}