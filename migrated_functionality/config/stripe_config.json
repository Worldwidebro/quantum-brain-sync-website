{
  "name": "stripe",
  "display_name": "Stripe",
  "description": "STRIPE MCP server for IZA OS ecosystem",
  "deployment": {
    "type": "docker",
    "image": "ghcr.io/klavis-ai/stripe-mcp-server:latest",
    "port": 5304,
    "environment": {
      "SERVER_NAME": "STRIPE",
      "IZA_OS_INTEGRATION": "true",
      "LOG_LEVEL": "info"
    }
  },
  "oauth": {
    "enabled": true,
    "scopes": [
      "read",
      "write"
    ],
    "redirect_uri": "https://iza-os.com/oauth/stripe/callback"
  },
  "capabilities": [
    "payment_processing",
    "subscription_management",
    "webhooks"
  ],
  "iza_os_integration": {
    "agent_access": [
      "execution_agent",
      "monitoring_agent"
    ],
    "workflow_integration": true,
    "monitoring_enabled": true
  }
}